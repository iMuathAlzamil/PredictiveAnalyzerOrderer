<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.predictiveaddordering">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.predictiveaddordering.HomeActivity" />
        <activity android:name="com.example.predictiveaddordering.ProductActivity" />
        <activity android:name="com.example.predictiveaddordering.ServiceActivity" />
        <activity android:name="com.example.predictiveaddordering.ProductViewActivity" />
        <activity android:name="com.example.predictiveaddordering.ServiceViewActivity" />
        <activity android:name="com.example.predictiveaddordering.ConfirmationActivity" />
        <activity android:name="com.example.predictiveaddordering.OrderActivity" />
        <activity android:name="com.example.predictiveaddordering.PromotionsActivity" />
    </application>

</manifest>